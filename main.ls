# Проверка интеллекта LavaScript
type "--- ТЕСТ ИНТЕЛЛЕКТА LS ---"

# Математика
molten a << 10
molten b << 20
molten result << (a + b) * 5
type "Результат (10 + 20) * 5 ="
type result

# Работа со строками
molten greeting << "Привет, "
molten user << "Создатель"
type greeting + user

type "--- ТЕСТ ЗАВЕРШЕН ---"
